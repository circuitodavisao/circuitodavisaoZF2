-- Table: fato_mensal

DROP TABLE fato_mensal;

CREATE TABLE fato_mensal
(
  id serial NOT NULL,
  numero_identificador character varying(64) NOT NULL,
  mes integer NOT NULL,
  ano integer NOT NULL,
  entidade character varying(64),
  lideres character varying(64),
  cu1 integer , 
  cu2 integer , 
  cu3 integer ,
  cu4 integer ,
  cu5 integer ,
  a1 integer ,
  a2 integer ,
  a3 integer ,
  a4 integer ,
  a5 integer ,
  d1 integer ,
  d2 integer ,
  d3 integer ,
  d4 integer ,
  d5 integer ,
  mem1 integer ,
  mem2 integer ,
  mem3 integer ,
  mem4 integer ,
  mem5 integer ,
  memp1 real ,
  memp2 real ,
  memp3 real ,
  memp4 real ,
  memp5 real ,
  mediamem real ,
  mediamemp real ,
--  pd1 integer ,
--  pd2 integer ,
--  pd3 integer ,
--  pd4 integer ,
--  pd5 integer ,
  c1 integer ,
  c2 integer ,
  c3 integer ,
  c4 integer ,
  c5 integer ,
  cp1 real ,
  cp2 real ,
  cp3 real ,
  cp4 real ,
  cp5 real ,
  mediac real ,
  mediacp real ,
  cq1 integer ,
  cq2 integer ,
  cq3 integer ,
  cq4 integer ,
  cq5 integer ,
  cqmeta1 integer ,
  cqmeta2 integer ,
  cqmeta3 integer ,
  cqmeta4 integer ,
  cqmeta5 integer ,
  cbq1 integer ,
  cbq2 integer ,
  cbq3 integer ,
  cbq4 integer ,
  cbq5 integer ,
  cbqmeta1 real ,
  cbqmeta2 real ,
  cbqmeta3 real ,
  cbqmeta4 real ,
  cbqmeta5 real ,
  realizada1 integer ,
  realizada2 integer ,
  realizada3 integer ,
  realizada4 integer ,
  realizada5 integer ,
  realizadap1 real ,
  realizadap2 real ,
  realizadap3 real ,
  realizadap4 real ,
  realizadap5 real ,
  mediarealizada real ,
  data_criacao date NOT NULL DEFAULT ('now'::text)::date, -- Data de criação do fato
  data_inativacao date, -- Data de inativação
  hora_inativacao time without time zone, -- Hora de inativação do fato
  hora_criacao time without time zone NOT NULL DEFAULT ('now'::text)::time with time zone, -- Hora de criação do fato
  CONSTRAINT pk_fato_mensal PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE fato_mensal
  OWNER TO postgres;

CREATE INDEX index_fato_mensal_numero_identificador
  ON fato_mensal
  USING btree
  (numero_identificador COLLATE pg_catalog."default");
